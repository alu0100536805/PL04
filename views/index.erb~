  <pre id=OUTPUT></pre>

  <script>
      function dump(fileName) {
          $.get(fileName, function (data) {
              $("#INPUT").val(data);
          });
      };
  </script>
  
  <div style="display:inline">
        <button id ="assignment" type="button" onclick="dump('assignment.txt');">Asignacion</button>
        <button id ="expression"  type="button" onclick="dump('expression.txt');">Expresion</button>
  </div>